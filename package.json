{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt prepare && nuxi typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue}\"",
    "lint:prettier": "prettier --check \"**/*.{ts,tsx,md,vue}\"",
    "generate-client": "pnpm openapi --input https://api.ratemanu.com/doc.json --output ./api-client/",
    "lint": "eslint --ignore-pattern './nuxt/*' && pnpm lint:prettier && pnpm run typecheck"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/eslint": "^1.0.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui": "^2.22.0",
    "@pinia/nuxt": "^0.11.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@vite-pwa/nuxt": "^0.10.8",
    "@vueuse/core": "^11.3.0",
    "nuxt": "^3.17.2",
    "nuxt-twemoji": "^3.7.1",
    "ohash": "^1.1.6",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "latest",
    "vue3-carousel-nuxt": "^1.1.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@openapitools/openapi-generator-cli": "^2.20.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^18.19.100",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.33.0",
    "ofetch": "^1.4.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.10"
  }
}
