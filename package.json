{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt prepare && nuxi typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue}\"",
    "lint:prettier": "prettier --check \"**/*.{ts,tsx,md,vue}\"",
    "generate-client": "pnpm openapi --input https://api.ratemanu.com/doc.json --output ./api-client/",
    "lint": "eslint --ignore-pattern './nuxt/*' && pnpm lint:prettier && pnpm run typecheck",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/eslint": "^0.6.2",
    "@nuxt/image": "^1.8.1",
    "@nuxt/ui": "^2.20.0",
    "@pinia/nuxt": "^0.5.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vueuse/core": "^11.3.0",
    "nuxt": "^3.17.2",
    "nuxt-twemoji": "^3.6.2",
    "ohash": "^1.1.4",
    "pinia-plugin-persistedstate": "^4.1.3",
    "vue": "latest",
    "vue3-carousel-nuxt": "^1.1.3",
    "yup": "^1.5.0"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.1",
    "@nuxtjs/storybook": "8.3.2",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@storybook/vue3": "8.4.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^18.19.67",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "ofetch": "^1.4.1",
    "prettier": "^3.4.2",
    "storybook": "8.4.2",
    "typescript": "^5.7.2",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.10"
  }
}
