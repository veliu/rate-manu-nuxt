{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt prepare && nuxi typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue}\"",
    "lint:prettier": "prettier --check \"**/*.{ts,tsx,md,vue}\"",
    "generate-client": "pnpm openapi --input https://api.ratemanu.com/doc.json --output ./api-client/",
    "lint": "eslint --ignore-pattern './nuxt/*' && pnpm lint:prettier && pnpm run typecheck"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
    "@nuxt/eslint": "^0.6.0",
    "@nuxt/image": "^1.8.1",
    "@nuxt/ui": "^2.18.7",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@vueuse/core": "^11.1.0",
    "nuxt": "^3.13.2",
    "nuxt-twemoji": "^3.6.2",
    "vue": "latest",
    "vue3-carousel-nuxt": "^1.1.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.1",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@openapitools/openapi-generator-cli": "^2.14.0",
    "@types/js-cookie": "^3.0.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.29.0",
    "ofetch": "^1.4.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6"
  }
}
